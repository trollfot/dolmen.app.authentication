<div class="management-panel"
     i18n:domain="dolmen.app.authentication">
  <h1 i18n:translate="">Authentication management: sources</h1>

  <div class="authentication-plugins">
    <h3 i18n:translate="">
      Active authentication sources
    </h3>
    <ul>
      <li tal:repeat="folder view/folders">
	<span tal:content="folder" />
      </li>
    </ul>
  </div>

  <div class="authentication-plugins">
    <h3 i18n:translate="">
      Active credentials sources
    </h3>
    <ul>
      <li tal:repeat="cred view/credentials">
	<span tal:content="cred" />
      </li>
    </ul>
  </div>

</div>
